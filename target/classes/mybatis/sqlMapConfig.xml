<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
	
	<typeAlias type="member.model.MemberBean" alias="MyMember"/>
 		<typeAlias type="category.model.BoardCateBean" alias="MyBoardCate"/>
 		<typeAlias type="category.model.ProdCateBean" alias="MyProdCate"/>
 		<typeAlias type="product.model.ProductBean" alias="MyProduct"/>
 		<typeAlias type="townboard.model.TownBoardBean" alias="MyTown"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/board.xml" />
		<mapper resource="mybatis/member.xml"/>
 		<mapper resource="mybatis/category.xml"/>
 		<mapper resource="mybatis/product.xml"/>
 		<mapper resource="mybatis/townboard.xml"/>
	</mappers>
</configuration>